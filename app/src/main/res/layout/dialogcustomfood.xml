<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:layout_margin="16dp">

    <TextView
        android:id="@+id/textView65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Tạo mới thức ăn"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Ednamedialogcustomfood"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_weight="8"
        android:background="@drawable/edit_text_bg"
        android:drawablePadding="10dp"
        android:hint="Tên món ăn"
        android:paddingStart="10dp"
        android:singleLine="true"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView65"
        android:layout_margin="10dp"/>

    <EditText
        android:id="@+id/Edcaloriedialogcustomfood"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:layout_marginTop="244dp"
        android:layout_weight="8"
        android:background="@drawable/edit_text_bg"
        android:drawablePadding="10dp"
        android:hint="Calore"
        android:paddingStart="10dp"
        android:singleLine="true"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Ednamedialogcustomfood" />


    <EditText
        android:id="@+id/Edproteindialogcustomfood"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:layout_marginTop="244dp"
        android:layout_weight="8"
        android:background="@drawable/edit_text_bg"
        android:drawablePadding="10dp"
        android:hint="Protein"
        android:paddingStart="10dp"
        android:singleLine="true"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edcaloriedialogcustomfood" />

    <EditText
        android:id="@+id/Edcarbdialogcustomfood"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:layout_marginTop="244dp"
        android:layout_weight="8"
        android:background="@drawable/edit_text_bg"
        android:drawablePadding="10dp"
        android:hint="Carbs"
        android:paddingStart="10dp"
        android:singleLine="true"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edproteindialogcustomfood" />

    <EditText
        android:id="@+id/Edfatsdialogcustomfood"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:layout_marginTop="244dp"
        android:layout_weight="8"
        android:background="@drawable/edit_text_bg"
        android:drawablePadding="10dp"
        android:hint="Fats"
        android:paddingStart="10dp"
        android:singleLine="true"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edcarbdialogcustomfood" />

    <Button
        android:layout_width="150dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edfatsdialogcustomfood"
        android:layout_margin="10dp"
        android:background="@drawable/btntieptheo"
        android:text="Tạo"
        app:backgroundTint="@color/blue"/>





</androidx.constraintlayout.widget.ConstraintLayout>